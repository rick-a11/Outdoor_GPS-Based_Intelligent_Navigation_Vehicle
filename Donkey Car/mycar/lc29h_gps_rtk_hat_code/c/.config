# config for SDK core
# Usage:
#       If a feature is required, modify its value to `y` or its own value, like `arm`, that is `CONFIG_XXX=arm`, to enable the feature.
#       If a new feature is finished, append the macro to this file.
# Notice:
#       DO NOT DIRECTLY MODIFY this file, copy and rename it, then modify it.
#

# for the system of target board
CONFIG_TARGET_SYS=unix

# for NOSR capability
CONFIG_NOSR=y
CONFIG_NOSR_DECODER=n

# for NSSR capability
CONFIG_NSSR=n
CONFIG_NSSR_DECODER=n

# for LSSR capability
CONFIG_LSSR=n
CONFIG_LSSR_DECODER=n

# for SIDS capability
CONFIG_SIDS=y
CONFIG_SIDS_DECODER=y

# for PDR capability
CONFIG_PDR=n

# for VDR capability
CONFIG_VDR=n

# for QX-SUPL(AGNSS) capability
CONFIG_QXSUPL=n

# for EPH capability
CONFIG_EPH=n

# for septentrio observation decoder
# CONFIG_SEP_OBS_DECODER=n

# for the bidirectional interaction between cloud and client in ARK
CONFIG_ARK=n

# for socket lifecycle control
CONFIG_SOCKET_LIFECYCLE=y

# for system memory allocator
CONFIG_MALLOC=y

# for memory allocator implemented in SDK
CONFIG_IMPL_DYNAMIC_MEMORY=n

# for implementation of platform related
CONFIG_IMPL_ASYNC_SOCKET=y
CONFIG_IMPL_MUTEXT_LOCK=y
CONFIG_IMPL_LOG_PRINT=y

# for daemon which works backgroud to drive tick periodically
CONFIG_DAEMON=y

# for AES
CONFIG_AES=y

# for cJSON
CONFIG_CJSON=y

# for BNC
CONFIG_BNC=n

# for coroutine
CONFIG_COROUTINE=n

# for coord sys
CONFIG_COORD_SYS=y

# for section charge
CONFIG_SECTION_CHARGE=y

# for ssl
CONFIG_SSL=n

# for boringssl
CONFIG_BORINGSSL=n

# for android logcat
CONFIG_ANDROID_LOG=n

# for terminal activation
CONFIG_TERMINAL_ACTIVATION=y

# for memory encrypt
CONFIG_MEMORY_ENCRYPT=n
